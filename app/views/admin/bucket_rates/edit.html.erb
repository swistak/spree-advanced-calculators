<h1><%= t("actions.editing")+" "+t("bucket_rate") %></h1>
<%= error_messages_for :bucket_rate %>
<% form_for(:bucket_rate, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("actions.update")%>
  </p>
<% end %>